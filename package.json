{
  "name": "@oredata/sdk",
  "version": "0.2.1",
  "description": "TypeScript client for the oredata.supply API.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "undici": "^6.22.0"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.5",
    "vitest": "^1.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oredata-supply/oredata-sdk.git"
  },
  "homepage": "https://oredata.supply",
  "bugs": {
    "url": "https://github.com/oredata-supply/oredata-sdk/issues"
  },
  "keywords": [
    "oredata",
    "solana",
    "ore",
    "mining",
    "api",
    "sdk"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18"
  }
}
